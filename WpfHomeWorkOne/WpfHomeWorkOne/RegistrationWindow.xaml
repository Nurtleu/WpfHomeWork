<Window x:Class="WpfHomeWorkOne.RegistrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfHomeWorkOne"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="RegistrationWindow" Height="292.381" Width="525">
    <Grid>
        <Label x:Name="labelLoginRegister" Content="Введите логин:" HorizontalAlignment="Left" Margin="21,35,0,0" VerticalAlignment="Top" FontSize="20" />
        <TextBox x:Name="textBoxLogin" HorizontalAlignment="Left" Height="30" Margin="266,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="190" FontSize="20" RenderTransformOrigin="0.324,1.857"/>
        <Button x:Name="buttonSave" Content="Сохранить" HorizontalAlignment="Left" Margin="370,213,0,12" Width="137" FontSize="20" Click="ButtonSave_Click"/>
        <Button x:Name="buttonClear" Content="Очистить" HorizontalAlignment="Left" Margin="217,213,0,12" Width="137" FontSize="20" Click="ButtonClear_Click"/>
        <Button x:Name="buttonTransition" Content="Авторизационный окно" HorizontalAlignment="Left" Margin="21,213,0,0" VerticalAlignment="Top" Width="182" Height="26" FontSize="16" Click="ButtonTransition_Click"/>
    </Grid>
</Window>
